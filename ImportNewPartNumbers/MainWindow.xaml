<Window x:Class="ImportNewPartNumbers.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        WindowStartupLocation="CenterScreen"
        WindowStyle="None"
        xmlns:local="clr-namespace:ImportNewPartNumbers"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="1000">
    <Grid>
        <DataGrid x:Name="dgrResults" HorizontalAlignment="Left" Height="772" Margin="10,10,0,0" VerticalAlignment="Top" Width="813" IsReadOnly="True" AlternationCount="1" RowBackground="AliceBlue" AlternatingRowBackground="LightBlue"/>
        <Button x:Name="btnImportExcel" Content="Import Excel" FontSize="20" HorizontalAlignment="Left" Margin="849,152,0,0" VerticalAlignment="Top" Width="133" Height="53" Click="btnImportExcel_Click"/>
        <Button x:Name="btnProcess" Content="Process" FontSize="20" HorizontalAlignment="Left" Margin="849,210,0,0" VerticalAlignment="Top" Width="133" Height="53" Click="btnProcess_Click"/>
        <Button x:Name="btnClose" Content="Close" FontSize="20" HorizontalAlignment="Left" Margin="849,268,0,0" VerticalAlignment="Top" Width="133" Height="53" Click="btnClose_Click"/>

    </Grid>
</Window>
